{
  "private": true,
  "version": "0.0.1",
  "name": "moon",
  "publisher": "moonrepo",
  "displayName": "moon",
  "description": "Control panel and console for moon.",
  "keywords": [
    "moon",
    "moonrepo",
    "build",
    "system",
    "ci",
    "cd",
    "console"
  ],
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "activationEvents": [
    "onCommand:moon.helloWorld"
  ],
  "main": "./lib/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moon.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "esbuild src/extension.ts --bundle --outfile=lib/extension.js --external:vscode --format=cjs --platform=node --target=node14",
    "watch": "npm run compile -- --sourcemap --watch",
    "package": ""
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "esbuild": "^0.15.10",
    "vsce": "^2.11.0"
  }
}
